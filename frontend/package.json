{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:fast": "next -c next.config.dev.js dev -p 3000",
    "dev:turbo": "next dev --turbo -p 3000",
    "dev:optimized": "node scripts/dev-optimization.js && next -c next.config.dev.js dev -p 3000",
    "dev:prod": "npm run build && npm run dev",
    "build": "next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "build:export": "NEXT_EXPORT=true next build && next export",
    "start": "cross-env NODE_ENV=production node server.js",
    "lint": "next lint",
    "deploy:firebase": "npm run build:export && firebase deploy --only hosting",
    "deploy:cloudrun": "gcloud builds submit --config cloudbuild-cloudrun.yaml"
  },
